<div class="content-heading">
  STOCK <%= @state %> <%= @brand %> <%= Date.today.strftime("%d %B %Y") %> REPORT for <%= @branch %>
</div>
<!-- START widgets box-->
<!-- END widgets box-->
<div class="row">
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <div class="row">
      <div class="col-lg-12">
        <div class="panel panel-info">
          <div class="panel-heading" style="text-align: center">
            Stock  
            <b>REAL TIME</b> 
          </div>
          <div class="panel-body">
            <table id="stock-table" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>PRODUCT</th>
                  <th>PANJANG</th>
                  <th class="sort-numeric">000</th>
                  <th class="sort-numeric">090</th>
                  <th class="sort-numeric">100</th>
                  <th class="sort-numeric">120</th>
                  <th class="sort-numeric">140</th>
                  <th class="sort-numeric">160</th>
                  <th class="sort-numeric">180</th>
                  <th class="sort-numeric">200</th>
                </tr>
              </thead>
              <tbody>
                <% @stock.each do |cs| %>
                <tr class="gradeX">
                  <td><%= cs.dsc1.strip + ' ' + cs.dsc2.strip[0..-10] %></td>
                  <td><%= cs.panjang %></td>
                  <td><%= cs.ah.to_i %></td>
                  <td><%= cs.aa.to_i %></td>
                  <td><%= cs.ab.to_i %></td>
                  <td><%= cs.ac.to_i %></td>
                  <td><%= cs.ad.to_i %></td>
                  <td><%= cs.ae.to_i %></td>
                  <td><%= cs.af.to_i %></td>
                  <td><%= cs.ag.to_i %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
          <div class="panel-footer">
            <ul>
              <li>DATA STOCK ADALAH JUMLAH YANG SIAP UNTUK DI BOOKING</li>
              <li>UNTUK UKURAN TANGGUNG ATAU LEBAR YANG TIDAK ADA DIATAS, SILAHKAN KLIK MENU TANGGUNG%></li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- END dashboard main content-->
</div>